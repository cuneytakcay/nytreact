{
  "name": "nytreact",
  "version": "1.0.0",
  "engines": {
    "node": "8.9.4",
    "npm": "5.7.1",
    "yarn": "1.5.1"
  },
  "main": "server.js",
  "repository": "git@github.com:cuneytakcay/nytreact.git",
  "author": "Cuneyt Akcay <cuneytakcay@gmail.com>",
  "license": "MIT",
  "scripts": {
    "server": "node server.js",
    "client": "cd client && npm run start",
    "start": "concurrently \"nodemon server.js\" \"npm run client\"",
    "build": "cd client && npm run build",
    "heroku-postbuild": "yarn build",
    "install": "(cd client && yarn) && yarn",
    "start:prod": "yarn start:prod"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "mongoose": "^5.0.12",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.2"
  }
}
